FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    object      B;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 0 -2 0 0 -1 0];

internalField   uniform (0.75 1 0);

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           uniform (0.75 1 0);
    }

    outlet
    {
        type            fixedValue;
        value           uniform (0.75 -1 0);
    }

    lowerWall
    {
        type            zeroGradient;
        //value           uniform (0.75 1 0);
    }

    upperWall
    {
        type            zeroGradient;
        //value           uniform (0.75 1 0);
    }

    frontWall
    {
        type            cyclic;
    }
    
    backWall
    {
        type            cyclic;
    }
}

